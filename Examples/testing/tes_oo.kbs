# 2008-09-06 jmr
# some samples from
# http://en.wikipedia.org/wiki/Order_of_operations
#
print "(4+10/2)/9 = 1 -- " + ((4+10/2)/9)
print "2 ^ (3 ^ 2) = 512 -- " + (2 ^ (3 ^ 2))
print "1+2*3 = 7 -- " + (1+2*3)
print "3 * 1 / 2 = 1.5 -- " + (3 * 1 / 2)
print "3 * 1 % 2 = 1 -- " + (3 * 1 % 2)
print "1 / 2 * 3 = 1.5 -- " + (1 / 2 * 3)
print "1 % 2 * 3 = 3 -- " + (1 % 2 * 3)
print "3 % 2 * 5 = 5 -- " + (3 % 2 * 5)
print "3 + 1 % 2 = 4 -- " + (3 + 1 % 2)
print "3 % 2 + 5 = 6 -- " + (3 % 2 + 5)